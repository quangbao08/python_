    "numbers = [80, 890, 345, 12]\n",
    "\n",
    "def get_min_numbers(numbers):\n",
    "    result = numbers[0]\n",
    "    \n",
    "    for num in numbers:\n",
    "        if result > num:\n",
    "            result = num\n",
    "    \n",
    "    return result\n",
    "\n",
    "min_number = get_min_numbers(numbers)\n",
    "print(\"Min number: \", min_number)"
   ]
